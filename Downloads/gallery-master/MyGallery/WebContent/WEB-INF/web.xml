<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<display-name>Gallery</display-name>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>greetme</servlet-name>
		<servlet-class>GreetMeHello</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>greetme</servlet-name>
		<url-pattern>/welcomedude</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>getmyimage</servlet-name>
		<servlet-class>database.GetMyImage</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>getmyimage</servlet-name>
		<url-pattern>/getmyimage</url-pattern>
	</servlet-mapping>

</web-app>